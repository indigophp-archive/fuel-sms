{
    "name": "indigophp/fuel-sms",
    "type": "fuel-package",
    "description": "Fuel wrapper around Indigo SMS package",
    "keywords": ["sms", "short-message", "gateway"],
    "homepage": "https://indigophp.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "composer/installers": "~1.0",
        "indigophp/fuel-core": "dev-develop",
        "indigophp/sms": "self.version"
    },
    "require-dev": {
        "mockery/mockery": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Sms\\": "classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sms\\Test\\": "tests/"
        }
    },
    "extra": {
        "installer-name": "sms"
    },
    "minimum-stability": "dev"
}
